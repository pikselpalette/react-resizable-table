{
  "name": "react-resizable-table",
  "description": "Add resizable columns to a table",
  "version": "0.1.0",
  "author": "Sam Boylett <sam.boylett@piksel.com>",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "webpack",
    "lint": "eslint lib && eslint test/spec",
    "flow": "./node_modules/.bin/flow",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:mutate": "NODE_ENV=test stryker run",
    "codecov": "codecov"
  },
  "jest": {
    "testMatch": [
      "**/test/spec/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.stryker-tmp/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**.*.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "lines": 100,
        "functions": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pikselpalette/react-resizable-table"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-react": "^7.9.1",
    "flow-babel-webpack-plugin": "^1.1.1",
    "flow-bin": "^0.73.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "semantic-ui-react": "^0.81.1",
    "stryker": "^0.23.0",
    "stryker-api": "^0.17.0",
    "stryker-babel-transpiler": "^0.5.0",
    "stryker-html-reporter": "^0.14.0",
    "stryker-javascript-mutator": "^0.7.0",
    "stryker-jest-runner": "^0.6.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-make-resizable": "^0.2.10"
  }
}
